<% if @user.errors.any?%>
  <% @user.errors.messages.each do |v|%>
    <%= v%>
  <%end%>
<%end%>
<table>
<%= form_for @user do |f|%>
<td><%= f.label :name %></td>
<td><%= f.text_field :name%></td>
</tr>
<tr>
<td><%= f.label :email %></td>
<td><%= f.text_field :email%></td>
</tr>
<tr>
<td><%= f.label :password %></td>
<td><%= f.password_field :password%></td>
</tr>

<%= f.fields_for :image do |ff|%>
<tr>
<td><%= f.label :photo %></td>
<td><%= ff.file_field :photo%></td>
</tr>
<%end%>
</tr>
<tr>
<td><%= f.submit %></td>
</tr>
<%end%>
</table>
